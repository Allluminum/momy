{
  "name": "momy",
  "version": "0.6.0",
  "description": "MongoDB to MySQL replication",
  "main": "index.js",
  "bin": {
    "momy": "./bin/momy.js"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "scripts": {
    "pretest": "mongodb-runner start --topology=replicaset --port=27017",
    "test": "npm run standard && npm run mocha",
    "posttest": "mongodb-runner stop --topology=replicaset --port=27017",
    "standard": "standard bin/*.js lib/*.js test/**/*.js",
    "mocha": "env TZ='Asia/Tokyo' mocha -t 20000 test/specs/*.js",
    "mocha-wo-db": "env TZ='Asia/Tokyo' mocha -t 20000 test/specs/defs.js",
    "try": "node ./bin/momy.js --config test/momyfile.json"
  },
  "dependencies": {
    "change-case": "^3.0.0",
    "moment": "^2.15.2",
    "mongodb": "^2.2.11",
    "mysql": "^2.12.0",
    "sqlstring": "^2.2.0"
  },
  "devDependencies": {
    "co": "^4.6.0",
    "mocha": "^3.1.2",
    "mongodb-runner": "^3.4.0",
    "standard": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cognitom/momy"
  },
  "author": "Tsutomu Kawamura",
  "license": "MIT"
}
